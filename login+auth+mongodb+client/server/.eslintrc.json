{
	"root": true,
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"settings": {
		"import/resolver": {
			"node": { "extensions": [".js", ".mjs"] }
		}
	},
	"rules": {
		"prettier/prettier": ["error"],
		"import/extensions": ["error", "always", { "js": "never", "mjs": "never" }]
	},
	"env": {
		"es6": true,
		"node": true
	}
}
