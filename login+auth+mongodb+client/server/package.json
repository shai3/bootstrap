{
	"name": "anyvision-server",
	"version": "0.1.0",
	"main": "src/index.mjs",
	"private": true,
	"author": "shai",
	"license": "MIT",
	"scripts": {
		"lint": "eslint ./src ./test --ext=.js,.mjs,.jsx --fix",
		"test": "jest --runInBand --forceExit",
		"start": "nodemon --preserve-symlinks --experimental-modules src/index.mjs"
	},
	"jest": {
		"testEnvironment": "node",
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"js",
			"mjs"
		],
		"transform": {
			"^.+\\.mjs$": "babel-jest"
		},
		"testRegex": "/test/.*.\\.test\\.mjs$",
		"setupTestFrameworkScriptFile": "./test/setupTestFrameworkScriptFile.mjs",
		"verbose": true
	},
	"dependencies": {
		"bcrypt": "^3.0.6",
		"@hapi/joi": "15.0.3",
		"@hapi/boom": "7.4.2",
		"dotenv": "8.0.0",
		"@hapi/hapi": "18.3.1",
		"hapi-auth-jwt2": "8.6.1",
		"jsonwebtoken": "9.0.0",
		"lodash": "4.17.11",
		"mongodb": "^3.2.4"
	},
	"devDependencies": {
		"@babel/core": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"babel-core": "7.0.0-bridge.0",
		"babel-jest": "^24.1.0",
		"nodemon": "^1.19.0",
		"eslint": "5.16.0",
		"eslint-config-airbnb-base": "13.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-import": "^2.17.2",
		"eslint-plugin-prettier": "^3.1.0",
		"jest": "^24.8.0",
		"prettier": "1.17.1"
	}
}
